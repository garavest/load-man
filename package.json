{
  "name": "@garavest/load-man",
  "version": "0.0.0-dev",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write . --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss",
    "lint": "npm run lint:code && npm run lint:format",
    "lint:code": "eslint .",
    "lint:format": "prettier -c . --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@prisma/client": "5.4.2",
    "@skeletonlabs/skeleton": "^2.3.0",
    "@sveltejs/kit": "^1.26.0",
    "luxon": "^3.4.3",
    "svelte": "^4.2.1",
    "sveltekit-superforms": "^1.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@garavest/eslint-config": "^2.0.1",
    "@garavest/prettier-config": "^1.1.0",
    "@playwright/test": "^1.39.0",
    "@prisma/engines": "^5.4.2",
    "@skeletonlabs/tw-plugin": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/google.maps": "^3.54.4",
    "@types/luxon": "^3.3.3",
    "@types/node": "^20.8.7",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "markdownlint": "^0.31.1",
    "markdownlint-cli": "^0.37.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.4.2",
    "svelte-check": "^3.5.2",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-tailwind-purgecss": "^0.1.3",
    "vitest": "^0.34.6"
  }
}
